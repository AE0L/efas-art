<div class="art-card card w-100 h-100" style="border: none;">
    <div class="art-img-cont">
        <img class="art-img h-100" src="<%= art_src %>" alt="">
    </div>
    <div class="card-body p-2 pt-3">
        <h5 class="card-title text-truncate" style="margin-bottom: 0;"><%= art_title %></h5>
    </div>
    <a href="/artworks/<%= art_id %>" class="stretched-link"></a>
</div>
