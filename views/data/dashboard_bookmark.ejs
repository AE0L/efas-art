<div class="art-card dash-art-card card w-100 h-100" style="border: none;">
    <div class="art-img-cont">
        <img class="art-img h-100" src="https://drive.google.com/uc?export=view&id=<%= art_pic %>" alt="">
    </div>
    <div class="card-body py-1 ps-2">
        <div class="row">
            <h4 class="card-title text-truncate mt-1 mb-2"><%= art_title %></h4>
        </div>
        <div class="row">
            <small class="card-title text-truncate text-muted" style="margin-bottom: 0;">@<%= art_user %></small>
        </div>
    </div>
    <a href="/artworks/<%= art_id %>?edit=true" class="stretched-link"></a>
</div>
